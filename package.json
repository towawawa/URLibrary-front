{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint:check": "eslint . --ext .js,.ts,.vue --ignore-path .gitignore --cache --cache-location node_modules/.cache/.eslintcache",
    "format:check": "prettier . --cache --check --ignore-path .gitignore",
    "type:check": "npx -y nuxi typecheck",
    "checkall": "yarn format:check && yarn lint:check && yarn type:check",
    "fix": "prettier . --cache --write --log-level warn --ignore-path .gitignore && yarn lint:check --fix && yarn type:check",
    "test": "vitest"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.6.0",
    "@nuxt/eslint-config": "^0.6.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unicorn": "^56.0.0",
    "nuxt": "^3.13.2",
    "prettier": "^3.3.3",
    "sass": "^1.80.4",
    "typescript": "^5.6.3",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^2.1.3",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.11",
    "@types/lodash-es": "^4.17.12",
    "dayjs": "^1.11.13",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "vue-select": "^4.0.0-beta.6"
  }
}